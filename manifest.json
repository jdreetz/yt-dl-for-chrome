{
  "name": "Youtube Downloader",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "This extension lets you download Youtube videos.",
  "homepage_url": "http://extensionizr.com",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/page_action/show_links.html"
  },
  "background": {
    "scripts": ["src/page_action/bootstrap.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "downloads"
  ]
}